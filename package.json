{
  "name": "openapi-parse",
  "version": "0.3.0",
  "description": "Basic parser for OpenAPI specs",
  "main": "index.js",
  "repository": "https://github.com/devdigital/openapi-parse.git",
  "author": "devdigital <devdigital.cloud@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "babel ./src --out-dir . --ignore __tests__",
    "watch": "babel ./src -w . --out-dir . --ignore __tests__",
    "test": "jest",
    "test:watch": "jest --verbose --watchAll ",
    "output": "babel-node run.js",
    "release": "standard-version"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "/node_modules/",
      ".cache"
    ],
    "verbose": true
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "inspected": "0.4.0",
    "json-schema-ref-parser": "5.1.3",
    "swagger2openapi": "3.2.10"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/node": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "cross-env": "5.2.0",
    "fs-extra": "^7.0.0",
    "jest": "23.6.0",
    "js-yaml": "3.12.0",
    "standard-version": "4.2.0"
  }
}
